{
  "name": "node-starter",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "description": "Start your Node.js project with this bulletproof boilerplate",
  "author": "https://github.com/antixrist",
  "repository": {
    "type": "git",
    "url": "https://github.com/antixrist/node-starter"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "check-eslint-prettier-rules-conflict": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "upgrade-deps": "npx npm-check-updates -u && npm ci",
    "lint": "eslint . --ext .js,.mjs",
    "lint:fix": "eslint . --ext .js,.mjs --fix && prettier \"**/*.{json,md}\" --write",
    "lint:staged": "lint-staged",
    "clean": "rm -rf dist && mkdir dist",
    "babel": "babel src --out-dir dist --copy-files --source-maps",
    "babel:watch": "run-s \"babel -- --watch\"",
    "watch": "run-s clean babel:watch",
    "build": "run-s clean babel",
    "dev": "node --stack-trace-limit=1024 -r ./bootstrap",
    "start": "cross-env NODE_ENV=production node --stack-trace-limit=1024 -r ./bootstrap",
    "migrate:make": "cross-env KNEX_CREATOR=1 knex migrate:make",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:currentVersion": "knex migrate:currentVersion",
    "migrate:latestVersion": "knex migrate:latestVersion",
    "seed:make": "cross-env KNEX_CREATOR=1 knex seed:make",
    "seed:run": "knex seed:run",
    "db:make": "run-s migrate:rollback migrate:latest seed:run",
    "test": "echo \"No test specified\" && exit 0"
  },
  "lint-staged": {
    "**/*.{js,mjs}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/runtime": "^7.5.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "clarify": "^2.1.0",
    "cross-env": "^5.2.0",
    "dotenv-safe": "^6.1.0",
    "ioredis": "^4.11.1",
    "knex": "^0.19.0",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.14",
    "make-promises-safe": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "objection": "^1.6.9",
    "objection-db-errors": "^1.1.1",
    "pg": "^7.11.0",
    "pg-query-stream": "^2.0.0",
    "signal-exit": "^3.0.2",
    "source-map-support": "^0.5.12",
    "trace": "^3.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-module-resolver": "^0.12.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-optimize-regex": "^1.1.6",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.4.0",
    "eslint-plugin-unicorn": "^9.1.1",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2"
  }
}
